#include <stdio.h>
#include "holberton.h"

/* function declaration of add_fib */
unsigned long long int add_fib(unsigned long long int num1, 
                               unsigned long long int num2);

/**
* Finds and prints first 50 fibonacci numbers
* Return: nothing
**/

void fib_nums()
{
	unsigned long long int num1 = 1;
	unsigned long long int num2 = 2;
	unsigned long long int fib_num;
	int count = 3;
	
	printf("%llu, %llu, ",num1, num2);
	while (count <= 50)
	{
		fib_num = add_fib(num1, num2);
		printf("fib num %d is %llu, ", count, fib_num);
		num1 = num2;
		num2 = fib_num;
	        
		count++;
	}
	printf("\n");
}

/* function returns the sum of two fib numbers */
unsigned long long int add_fib(unsigned long long int num1, 
			       unsigned long long int num2)
{
	unsigned long long int sum;
	
	sum = num1 + num2;

	return sum;
}
 
